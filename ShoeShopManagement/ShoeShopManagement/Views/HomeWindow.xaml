<Window x:Class="ShoeShopManagement.Views.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:local="clr-namespace:ShoeShopManagement.Resources.UserControls"
        xmlns:Wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        x:Name="wdHomeWindow"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        WindowStartupLocation="CenterScreen"
        Title="HomeWindow" Height="800" Width="1400"
        Icon="/ShoeShopManagement;component/Resources/Image/Logo.jpg"
        DataContext="{StaticResource HomeVM}">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resources/Image/background.png">
            </ImageBrush>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="10*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Row="0"
              Background="#E8641A"
              HorizontalAlignment="Left"
              Margin="10,10,5,0">
            <StackPanel Width="200"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                <StackPanel.Resources>
                    <Style TargetType="materialDesign:PackIcon">
                        <Setter Property="VerticalAlignment" Value="Center"></Setter>
                        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
                        <Setter Property="Height" Value="40"></Setter>
                        <Setter Property="Width" Value="40"></Setter>
                        <Setter Property="Foreground" Value="White"/>
                    </Style>
                    <Style TargetType="Grid">
                        <Setter Property="Height" Value="60"></Setter>
                    </Style>
                </StackPanel.Resources>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon
                        x:Name="iconHome"
                        Grid.Column="0" 
                        Kind="Home"/>
                    <Button Content="Trang chủ"
                            x:Name="btnHome"
                            Style="{StaticResource MainMenuButton}"
                            Uid ="0">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnHome}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="GoogleMyBusiness" 
                        Grid.Column="0" 
                        x:Name="iconBusiness"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            x:Name="btnBusiness"
                            Content="Bán hàng"
                            Uid ="1">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnBusiness}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="Store" 
                        Grid.Column="0" 
                        x:Name="iconService"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Dịch vụ"
                            x:Name="btnService"
                            Uid ="2">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnService}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="PackageVariant" 
                        Grid.Column="0" 
                        x:Name="iconGood"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Hàng hóa"
                            x:Name="btnGoods"
                            Uid ="3">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnGoods}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="AccountCowboyHat" 
                        Grid.Column="0" 
                        x:Name="iconCustomer" />
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Khách hàng"
                            x:Name="btnCustomer"
                            Uid ="4">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnCustomer}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="FileChart" 
                        Grid.Column="0" 
                        x:Name="iconReport"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Báo cáo"
                            x:Name="btnReport"
                            Uid ="5">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnReport}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="Account" 
                        Grid.Column="0"
                        x:Name="iconEmployee"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Nhân viên"
                            x:Name="btnEmployee"
                            Uid ="6">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnEmployee}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon 
                        Kind="Account" 
                        Grid.Column="0"
                        x:Name="iconStock"/>
                    <Button Style="{StaticResource MainMenuButton}"
                            Content="Kho"
                            x:Name="btnStock"
                            Uid ="7">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding SwitchTabCommand}"
                                                       CommandParameter="{Binding ElementName=wdHomeWindow}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                            <i:EventTrigger EventName="MouseMove">
                                <i:InvokeCommandAction Command="{Binding GetNameTab}"
                                                       CommandParameter="{Binding ElementName=btnStock}"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </Button>
                </Grid>
            </StackPanel>
            <Grid 
                x:Name="grdCursor" 
                Width="4" 
                Height="50" 
                Background="{DynamicResource TextMainColorButton}" 
                HorizontalAlignment="Left"
                Margin="0,145,35,0" 
                VerticalAlignment="Top"/>
            <Grid Width="auto"
                  Height="auto"
                  Margin="50 35"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Top">
                <Image  Source="/Resources/Image/logo.png" 
                       Stretch="Uniform"></Image>
            </Grid>
            <Grid Width="180"
                  Height="50"
                  HorizontalAlignment="Right"
                  VerticalAlignment="Bottom"
                  Margin="0,0,42,16.4">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.Resources>
                    <Style TargetType="materialDesign:PackIcon">
                        <Setter Property="VerticalAlignment" Value="Center"></Setter>
                        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
                        <Setter Property="Height" Value="30"></Setter>
                        <Setter Property="Width" Value="30"></Setter>
                    </Style>
                </Grid.Resources>
                <materialDesign:PackIcon Kind="LogoutVariant" Grid.Column="0" Foreground="White" Width="50" Height="50" />
                <Button Style="{StaticResource MainMenuButton}"
                            Content="Đăng xuất"
                        Foreground="#FEECEC"
                            FontSize="22"
                            FontWeight="Medium"
                            Panel.ZIndex="1" Background="{x:Null}"
                        ></Button>
            </Grid>
        </Grid>
        <Grid Grid.Column="1"
              Margin="0 0 10 0">
            <Grid.RowDefinitions>
                <RowDefinition Height="1.5*"></RowDefinition>
                <RowDefinition Height="12*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid Height="Auto"
                      Margin="5"
                      HorizontalAlignment="Right">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="1*"></ColumnDefinition>
                        <ColumnDefinition Width="auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="2"
                          VerticalAlignment="Top"
                          Margin="0,20">
                        <Button materialDesign:ButtonAssist.CornerRadius="10"
                                Height="40"
                                Width="40"
                                Background="#C4C4C4">
                        </Button>
                    </Grid>
                    <Grid Grid.Column="1">
                        <TextBlock Text="Huy Hoàng"
                                   FontSize="25"
                                   FontFamily="Roboto"
                                   FontWeight="Regular"
                                   VerticalAlignment="Top"
                                   Margin="0,25,10,0"
                                   Foreground="White"
                                   />
                    </Grid>
                    <Grid 
                        Grid.Row="0" 
                        VerticalAlignment="Center"
                        Margin="0,0,20,0">
                        <Button
                        Height="50" 
                        Width="150" 
                        VerticalAlignment="Center"
                        FontSize="20"
                        Content="Điểm danh"
                        materialDesign:ButtonAssist.CornerRadius="5" BorderBrush="{x:Null}">
                            <Button.Background>
                                <ImageBrush ImageSource="/Resources/Image/btnImage.png"></ImageBrush>
                            </Button.Background>
                        </Button>
                    </Grid>
                </Grid>
            </Grid>
            <Grid
                x:Name="grdBody_Home"
                Background="#EFD0BF"
                Grid.Row="1" 
                DataContext="{StaticResource ReportVM}"
                Visibility="Visible">
                <Grid.RowDefinitions>
                    <RowDefinition Height="200"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="Loaded">
                        <i:InvokeCommandAction Command="{Binding LoadCommand}"
                                                CommandParameter="{Binding ElementName=wdHomeWindow}"/>
                    </i:EventTrigger>
                </i:Interaction.Triggers>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <materialDesign:Card
                        Padding="4"
                        Margin="20 20 20 20"
                        Foreground="{x:Null}"
                        Grid.Column="0">
                        <materialDesign:Card.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF7D27FF" Offset="1"/>
                                <GradientStop Color="#FFBB2086"/>
                            </LinearGradientBrush>
                        </materialDesign:Card.Background>
                        <Grid>
                            <Label 
                                x:Name="label" 
                                Content="Tổng doanh thu tháng này" 
                                HorizontalAlignment="Left"
                                Margin="10"
                                VerticalAlignment="Top" FontSize="20" 
                                Foreground="#DDF9F9F9"/>
                            <Label 
                                x:Name="label_total_money_business" 
                                Content="{Binding ThisMonthMoney}" 
                                HorizontalAlignment="Left"
                                Margin="10 70 0 0" 
                                VerticalAlignment="Top"
                                FontSize="24" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label 
                                Content="{Binding ThisMonth}" 
                                HorizontalAlignment="Left"
                                Margin="10 100 0 0" 
                                VerticalAlignment="Bottom"
                                FontSize="18" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label x:Name="label_image_business"
                               Content="" 
                               HorizontalAlignment="Right"
                               Height="60"
                               Margin="10 10"
                               VerticalAlignment="Top" Width="60">
                                <Label.Background>
                                    <ImageBrush ImageSource="/ShoeShopManagement;component/Resources/Image/money.png" Stretch="Uniform"/>
                                </Label.Background>
                            </Label>
                        </Grid>
                    </materialDesign:Card>
                    <materialDesign:Card
                        Padding="4"
                        Margin="20 20 20 20"
                        Foreground="{x:Null}"
                        Grid.Column="1">
                        <materialDesign:Card.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFF51C40"/>
                                <GradientStop Color="#FFD89B65" Offset="1"/>
                            </LinearGradientBrush>
                        </materialDesign:Card.Background>
                        <Grid>
                            <Label 
                                x:Name="label_service" 
                                Content="So với tháng trước" 
                                HorizontalAlignment="Left"
                                Margin="10"
                                VerticalAlignment="Top" FontSize="20" 
                                Foreground="#DDF9F9F9"/>
                            <Label 
                                x:Name="label_total_money_service" 
                                Content="{Binding IncreasingPercent}" 
                                HorizontalAlignment="Left"
                                Margin="10 70 0 0" 
                                VerticalAlignment="Top"
                                FontSize="24" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label 
                                Content="{Binding ThisMonth}" 
                                HorizontalAlignment="Left"
                                Margin="10 100 0 0" 
                                VerticalAlignment="Bottom"
                                FontSize="18" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label x:Name="label_image_service"
                               Content="" 
                               HorizontalAlignment="Right"
                               Height="60"
                               Margin="10 10"
                               VerticalAlignment="Top" Width="60">
                                <Label.Background>
                                    <ImageBrush ImageSource="/ShoeShopManagement;component/Resources/Image/IncreaseIcon.png" Stretch="Uniform"/>
                                </Label.Background>
                            </Label>
                        </Grid>
                    </materialDesign:Card>
                    <materialDesign:Card
                        Padding="4"
                        Margin="20 20 20 20"
                        Foreground="{x:Null}"
                        Grid.Column="2">
                        <materialDesign:Card.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF016CFD" Offset="1"/>
                                <GradientStop Color="#FF00E4FF"/>
                            </LinearGradientBrush>
                        </materialDesign:Card.Background>
                        <Grid>
                            <Label 
                                x:Name="label_three" 
                                Content="Số lượt đặt hàng và dịch vụ" 
                                HorizontalAlignment="Left"
                                Margin="10"
                                VerticalAlignment="Top" FontSize="20" 
                                Foreground="#DDF9F9F9"/>
                            <Label 
                                x:Name="label_total_money_three" 
                                Content="{Binding NumOfBuy}" 
                                HorizontalAlignment="Left"
                                Margin="10 70 0 0" 
                                VerticalAlignment="Top"
                                FontSize="24" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label 
                                Content="{Binding ThisMonth}" 
                                HorizontalAlignment="Left"
                                Margin="10 100 0 0" 
                                VerticalAlignment="Bottom"
                                FontSize="18" 
                                Foreground="#DDF9F9F9" 
                                FontWeight="Bold"/>
                            <Label x:Name="label_image_three"
                               Content="" 
                               HorizontalAlignment="Right"
                               Height="60"
                               Margin="10 10"
                               VerticalAlignment="Top" Width="60">
                                <Label.Background>
                                    <ImageBrush ImageSource="/ShoeShopManagement;component/Resources/Image/IncreaseIcon.png" Stretch="Uniform"/>
                                </Label.Background>
                            </Label>
                        </Grid>
                    </materialDesign:Card>
                </Grid>
                <Grid
                    Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="380"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <materialDesign:Card
                        Padding="8"
                        Margin="20 20 20 20"
                        Grid.Column="0">
                        <Wpf:PieChart
                            x:Name="lvcPieChart"
                            DataContext="{StaticResource ReportVM}"
                            LegendLocation="None"
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Top"
                            Width="300"
                            Height="320"
                            Margin="0 50 0 0"
                            Hoverable="True"
                            HoverPushOut="10"
                            FontSize="16"
                            Series="{Binding PieSeriesCollection}">
                        </Wpf:PieChart>
                    </materialDesign:Card>
                    <ComboBox
                        x:Name="cboSelectTimePie"
                        Grid.Column="0"
                        materialDesign:ComboBoxAssist.ClassicMode="true"
                        FontSize="16"
                        FontWeight="Medium" 
                        Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                        materialDesign:HintAssist.Hint="Chọn thời gian"
                        Width="140" 
                        Height="45" 
                        HorizontalAlignment="Left" 
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center" 
                        Padding="3,0,0,0" 
                        Margin="30,25,0,0"
                        VerticalAlignment="Top">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="SelectionChanged">
                                <i:InvokeCommandAction Command="{Binding InitPieChartCommand}" 
                                                CommandParameter="{Binding ElementName=wdHomeWindow}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                        <ComboBoxItem Content="Hôm nay"/>
                        <ComboBoxItem Content="Tháng nay"/>
                    </ComboBox>
                    <StackPanel 
                        Orientation="Horizontal"
                        HorizontalAlignment="Center"
                        Height="45"
                        Margin="0,400,0,0" 
                        VerticalAlignment="Top"
                        Grid.Column="0">
                        <ToggleButton
                                Style="{StaticResource MaterialDesignActionToggleButton}"
                                ToolTip="Sân"
                                Width="45" 
                                Height="45" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top"
                                Margin="0" 
                                Background="{DynamicResource Color2}">
                            <materialDesign:ToggleButtonAssist.OnContent>
                                <materialDesign:PackIcon 
                                        Kind="Soccer"
                                        Width="30"
                                        Height="30" 
                                        Foreground="{DynamicResource White}"/>
                            </materialDesign:ToggleButtonAssist.OnContent>
                            <materialDesign:PackIcon 
                                        Kind="Soccer"
                                        Width="30"
                                        Height="30" 
                                        Foreground="{DynamicResource White}" />
                        </ToggleButton>
                        <TextBlock
                                TextWrapping="Wrap" 
                                Text="Bán hàng" 
                                Margin="8,0,16,0" 
                                FontSize="16" 
                                FontWeight="SemiBold" 
                                VerticalAlignment="Center"/>
                        <ToggleButton
                                Style="{StaticResource MaterialDesignActionToggleButton}"
                                ToolTip="Sân"
                                Width="45" 
                                Height="45" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Top"
                                Margin="0" 
                                Background="{DynamicResource PaletteColor3}">
                            <materialDesign:ToggleButtonAssist.OnContent>
                                <materialDesign:PackIcon 
                                        Kind="ShoppingOutline"
                                        Width="30"
                                        Height="30" 
                                        Foreground="{DynamicResource White}" />
                            </materialDesign:ToggleButtonAssist.OnContent>
                            <materialDesign:PackIcon 
                                        Kind="ShoppingOutline"
                                        Width="30"
                                        Height="30" 
                                        Foreground="{DynamicResource White}" />
                        </ToggleButton>
                        <TextBlock
                                TextWrapping="Wrap" 
                                Text="Dịch vụ" 
                                Margin="8,0" 
                                FontSize="16"
                                FontWeight="SemiBold" 
                                VerticalAlignment="Center"/>
                    </StackPanel>
                    <Grid
                        Grid.Column="1">
                        <materialDesign:Card
                        Padding="8"
                        Margin="20 20 20 20"
                        Grid.Column="0">
                            <Wpf:CartesianChart 
                                x:Name="lvcColumnChart" 
                                DataContext="{StaticResource ReportVM}"
                                Series="{Binding SeriesCollection}" 
                                LegendLocation="Bottom"
                                Margin="0 50 0 0">
                                <Wpf:CartesianChart.AxisX>
                                    <Wpf:Axis 
                                    Title="{Binding AxisXTitle, Mode=TwoWay}" 
                                    Labels="{Binding Labels, Mode=TwoWay}" 
                                    Foreground="{DynamicResource Color1}"
                                    FontSize="14">
                                        <Wpf:Axis.Separator>
                                            <Wpf:Separator Step="1"/>
                                        </Wpf:Axis.Separator>
                                    </Wpf:Axis>
                                </Wpf:CartesianChart.AxisX>
                                <Wpf:CartesianChart.AxisY>
                                    <Wpf:Axis 
                                    Title="{Binding AxisYTitle, Mode=TwoWay}" 
                                    LabelFormatter="{Binding Formatter}"
                                    Foreground="{DynamicResource Color1}"
                                    FontSize="14"/>
                                </Wpf:CartesianChart.AxisY>
                            </Wpf:CartesianChart>
                        </materialDesign:Card>
                    </Grid>
                    <StackPanel 
                        Orientation="Horizontal"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Center" 
                        Margin="0,25,0,0"
                        Grid.Column="1">
                        <ComboBox
                            DataContext="{StaticResource ReportVM}"
                            x:Name="cboSelectPeriod"
                            materialDesign:ComboBoxAssist.ClassicMode="true"
                            FontSize="16"
                            FontWeight="Medium" 
                            Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                            materialDesign:HintAssist.Hint="Chọn chu kỳ"
                            Width="160" 
                            Height="40" 
                            HorizontalAlignment="Left" 
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center" 
                            Padding="3,0,0,0" 
                            Margin="0">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding SelectionChangedCommand}" 
                                           CommandParameter="{Binding ElementName=wdHomeWindow}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <ComboBoxItem Content="Theo tháng"/>
                            <ComboBoxItem Content="Theo quý"/>
                            <ComboBoxItem Content="Theo năm"/>
                        </ComboBox>
                        <ComboBox
                                x:Name="cboSelectTime"
                                DataContext="{StaticResource ReportVM}"
                                ItemsSource="{Binding ItemSourceTime, Mode=TwoWay}"
                                materialDesign:ComboBoxAssist.ClassicMode="true"
                                FontSize="16"
                                FontWeight="Medium" 
                                Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                materialDesign:HintAssist.Hint="Chọn"
                                Width="160" 
                                Height="40" 
                                HorizontalAlignment="Left" 
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Center" 
                                Padding="3,0,0,0" 
                                Margin="60,0,0,0" >
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="SelectionChanged">
                                    <i:InvokeCommandAction Command="{Binding InitColumnChartCommand}" 
                                           CommandParameter="{Binding ElementName=wdHomeWindow}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </ComboBox>
                    </StackPanel>
                </Grid>
            </Grid>
            <Grid  
                HorizontalAlignment="Left" 
                Height="710" 
                VerticalAlignment="Top" 
                Background="#EFD0BF"
                Width="1160"
                Grid.Row="1" 
                x:Name="grdBody_Business"
                DataContext="{StaticResource EmployeeVM}"
                Visibility="Hidden">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="70*"></ColumnDefinition>
